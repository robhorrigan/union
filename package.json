{
  "name": "@xo-union/union",
  "version": "0.0.1",
  "description": "",
  "main": "webpack.config.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/xogroup/union"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=7.0.0 <8.0"
  },
  "scripts": {
    "benchmark": "yarn run karma -- start karma.conf.benchmark.js",
    "bootstrap": "lerna bootstrap --hoist && npm link ./packages/xojs",
    "bootstrap.generators": "npm link ./packages/tools/generator-union",
    "build": "yarn run build.patterns && yarn run build.docs && yarn run build.xojs",
    "build.docs": "webpack --config webpack.docs.babel.js",
    "build.patterns": "webpack --config webpack.patterns.babel.js",
    "build.xojs": "babel packages/xojs/src --out-dir packages/xojs/lib --ignore *spec.js,*benchmark.js",
    "clean": "yarn run clean.dependencies && yarn run clean.build",
    "clean.all": "yarn run clean && rm -rf node_modules",
    "clean.build": "git clean -dfx packages/@xo-union packages/xojs",
    "clean.public": "git clean -dfx public/",
    "clean.dependencies": "echo y | lerna clean",
    "eslint": "eslint",
    "karma": "yarn run node -- $(which karma)",
    "lint": "yarn run node -- ./scripts/lint/index.js",
    "login": "sh ./scripts/login.sh",
    "logs": "pm2 logs --lines=40",
    "new.pattern": "yo union:pattern",
    "node": "node -r babel-register",
    "open": "open http://localhost:8080",
    "pm2": "pm2",
    "prebuild.patterns": "yarn run build.xojs",
    "publish.assets": "sh ./scripts/publish-assets.sh",
    "publish.docs": "sh ./scripts/publish-docs.sh",
    "publish.packages": "sh ./scripts/publish-packages.sh",
    "publish.packages.cherrypick": "sh ./scripts/publish-packages-cherrypick.sh",
    "restart": "pm2 restart all",
    "setup": "yarn install && yarn run bootstrap && yarn run build.patterns",
    "start": "pm2 start devprocesses.config.js",
    "start.docserver": "webpack-dev-server --inline --hot --config webpack.docs.babel.js --compress",
    "status": "pm2 status",
    "stop": "pm2 stop all",
    "test": "yarn run test.browser && yarn run test.node && yarn run lint",
    "test.browser": "yarn run karma -- start --single-run",
    "test.browser.chrome": "yarn run karma -- start --browsers=ChromeWithDevTools --single-run",
    "test.browser.watch": "yarn run karma -- start",
    "test.node": "NODE_PATH=packages yarn run node -- ./scripts/node-tests.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.1",
    "css-module-builder": "^1.0.0-beta.5",
    "humanize-string": "https://registry.npmjs.org/humanize-string/-/humanize-string-1.0.1.tgz",
    "jsx-to-string-2": "^1.0.0",
    "karma-benchmark-reporter": "^0.1.1",
    "marksy": "^0.3.0",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "mobx-react-router": "^3.1.2",
    "pascal-case": "^2.0.0",
    "prism-themes": "^1.0.0",
    "prismjs": "^1.6.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-css-modules": "^4.2.1",
    "react-dom": "^15.5.4",
    "react-router": "^3.0.5",
    "split-array": "^1.0.1",
    "xojs": "^0.0.4"
  },
  "devDependencies": {
    "a-css-loader": "^1.0.0-beta.11",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "4.0.0-alpha.6",
    "chalk": "https://registry.npmjs.org/chalk/-/chalk-1.1.3.tgz",
    "classnames": "^2.2.5",
    "css": "^2.2.1",
    "css-byebye": "^1.1.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "exports-loader": "https://registry.npmjs.org/exports-loader/-/exports-loader-0.6.3.tgz",
    "extend": "^3.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "glob-all": "^3.1.0",
    "html-webpack-plugin": "^2.26.0",
    "image-webpack-loader": "^3.3.1",
    "imports-loader": "^0.7.1",
    "inquirer": "^3.0.6",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^4.1.0",
    "jsx-to-string-2": "^1.0.0",
    "karma": "^1.6.0",
    "karma-benchmark": "^0.7.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.3",
    "lerna": "^2.0.0-rc.5",
    "loader-utils": "^1.1.0",
    "microtime": "^2.1.3",
    "node-promisify": "^1.2.0",
    "node-sass": "^4.5.3",
    "pascal-case": "^2.0.0",
    "pm2": "^2.4.5",
    "postcss-loader": "^1.3.3",
    "postcss-remove-rules": "^1.0.0",
    "prism-themes": "^1.0.0",
    "raw-loader": "^0.5.1",
    "react-docgen": "^2.14.1",
    "react-docgen-loader": "^2.9.1",
    "react-markdown-loader": "^1.1.6",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^4.1.1",
    "semver": "https://registry.npmjs.org/semver/-/semver-5.3.0.tgz",
    "string-loader": "^0.0.1",
    "style-loader": "^0.13.2",
    "svgo-loader": "^1.2.1",
    "tar-stream": "^1.5.2",
    "treeify": "^1.0.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4",
